function bouncer(arr) {
  // Don't show a false ID to this bouncer.
  let newArr = [];
  for (let index in arr)
  {
    if (!(arr[index])){}
    else
    {
      newArr.push(arr[index]);
    }
  }
  return newArr;
}

console.log(bouncer([7, "ate", "", false, 9]));
console.log(bouncer([false, null, 0, NaN, undefined, ""]))
